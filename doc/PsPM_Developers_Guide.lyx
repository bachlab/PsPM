\end_layout

\begin_layout Standard
Setup MATLAB
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

- name: Setup MATLAB
\end_layout

\begin_layout Plain Layout

  uses: matlab-actions/setup-matlab@v1.0.1
\end_layout

\end_inset


\end_layout

\begin_layout Subparagraph
Step 4
\end_layout

